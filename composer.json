{
  "name": "longitude-one/property-bundle",
  "description": "A Symfony bundle to dynamically add properties to objects",
  "type": "bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/http-kernel": "^6.1",
    "symfony/form": "^6.1",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "LongitudeOne\\PropertyBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LongitudeOne\\PropertyBundle\\Tests\\": "tests/",
      "LongitudeOne\\PropertyBundle\\Tests\\App\\": "tests/App/src"
    }
  },
  "require-dev": {
    "symfony/framework-bundle": "^6.1",
    "symfony/yaml": "^6.1",
    "symfony/console": "^6.1",
    "symfony/process": "^6.1",
    "phpunit/phpunit": "^8.5|^9.5",
    "symfony/browser-kit": "^6.1",
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpstan/phpstan-doctrine": "^1.3",
    "symfony/test-pack": "^1.0"
  }
}
