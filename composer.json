{
  "name": "longitude-one/property-bundle",
  "description": "A Symfony bundle to dynamically add properties to objects",
  "type": "bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/http-kernel": "^6.1",
    "symfony/form": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "LongitudeOne\\PropertyBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LongitudeOne\\PropertyBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/framework-bundle": "^6.1",
    "symfony/yaml": "^6.1",
    "symfony/console": "^6.1",
    "symfony/process": "^6.1",
    "phpunit/phpunit": "^8.5|^9.5",
    "symfony/browser-kit": "^6.1",
    "jetbrains/phpstorm-attributes": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
