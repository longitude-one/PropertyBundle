{
  "name": "longitude-one/cookies-consent-bundle",
  "description": "A Symfony bundle to manage cookies with respect and morality (and bootstrap 5.1)",
  "type": "bundle",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "symfony/http-kernel": "^4.4|^5.4|^6.0"
  },
  "autoload": {
    "psr-4": {
      "LongitudeOne\\CookiesConsentBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LongitudeOne\\CookiesConsentBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/framework-bundle": "^4.4|^5.4|^6.0",
    "symfony/yaml": "^4.4|^5.4|^6.0",
    "symfony/console": "^4.4|^5.4|^6.0",
    "symfony/process": "^4.4|^5.4|^6.0",
    "phpunit/phpunit": "^8.5|^9.5",
    "symfony/browser-kit": "^4.4|^5.4|^6.0"
  }
}
